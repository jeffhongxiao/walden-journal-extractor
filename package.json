{
  "name": "walden-extractor",
  "version": "1.0.0",
  "description": "Journal extractor for waldengame.com",
  "productName": "Walden Journal Extractor",
  "main": "./dist/gui/main",
  "scripts": {
    "start": "tsc --watch",
    "postinstall": "tsc",
    "gui": "electron .",
    "package": "npm run package-win && npm run package-osx",
    "package-win": "electron-packager . --overwrite --out=release-builds --platform=win32 --arch=ia32 --icon=icon.ico",
    "package-osx": "electron-packager . --overwrite --out=release-builds --platform=darwin --arch=x64 --icon=icon.icns",
    "package-osx-in-docker": "docker-compose run app",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Atul Varma <varmaa@gmail.com>",
  "license": "CC0-1.0",
  "dependencies": {
    "@types/react-transition-group": "^2.0.7",
    "docx": "^3.2.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-transition-group": "^2.2.1",
    "slugify": "^1.2.9",
    "source-map-support": "^0.5.3",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "@types/react": "^16.0.38",
    "@types/react-dom": "^16.0.4",
    "@types/xml2js": "^0.4.2",
    "electron": "^1.8.2",
    "electron-packager": "^11.0.1",
    "typescript": "^2.7.2"
  }
}
